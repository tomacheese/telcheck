{
  "name": "telcheck",
  "version": "2.0.0",
  "description": "Check caller and notify to discord for NVR510",
  "main": "dist/main.js",
  "repository": "git@github.com:tomacheese/telcheck.git",
  "author": "Tomachi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/ -m",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "clean": "rimraf dist output",
    "generate-schema": "typescript-json-schema --required tsconfig.json Configuration -o schema/Configuration.json",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@book000/node-utils": "1.12.88",
    "@fastify/basic-auth": "5.1.1",
    "@fastify/cors": "9.0.1",
    "@types/cheerio": "0.22.35",
    "@types/node": "20.11.26",
    "@types/web-push": "3.6.3",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vercel/ncc": "0.38.1",
    "axios": "1.6.7",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "51.0.1",
    "fastify": "4.26.2",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.2",
    "typescript-json-schema": "0.63.0",
    "web-push": "3.6.7",
    "yarn-run-all": "3.1.1"
  }
}
